version=@DENGINE_INIT_CONF_VERSION@
[window]
window_width=854
window_height=480

# would you like screen multisampled anti aliasing?
window_msaa=4

#vsync
window_swapinterval=1
 
[fontconfigs]
# fontsize in pixel size
font_size=18.0
# size of the initial font bitmap
font_bitmapsize=512
 
[caching]
# cache textures to disk. may improve loading speeds at the cost of higher disk usage
cache_textures=0
# cache shaders to disk. always use this option if your hardware supports it
cache_shaders=1
