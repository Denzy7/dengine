prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@
libdir=@CMAKE_INSTALL_FULL_LIBDIR@

Name: Dengine
Description: Lightweight Graphics Engine
Version: @DENGINE_VERSION@
URL: https://github.com/Denzy7/dengine
Requires.private: @DENGINE_PKG_DEPS@
Libs: -L${libdir} @DENGINE_PKG_LIBS@
Cflags: -I${includedir} -I@CMAKE_INSTALL_FULL_INCLUDEDIR@/dengine-@DENGINE_VERSION@/lib -I@CMAKE_INSTALL_FULL_INCLUDEDIR@/dengine-@DENGINE_VERSION@/deps -I@CMAKE_INSTALL_FULL_INCLUDEDIR@/dengine-@DENGINE_VERSION@/dengine_config
