prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@
libdir=@CMAKE_INSTALL_FULL_LIBDIR@

Name: Dengine
Description: Simple 3D Graphics Engine
Version: @DENGINE_VERSION@
URL: https://github.com/Denzy7/dengine
Requires.private: @DENGINE_PKG_DEPS@
Libs: -L${libdir} -L${libdir}/dengine-@DENGINE_VERSION@ @DENGINE_PKG_LIBS@
Libs.private: -lm
Cflags: -I@CMAKE_INSTALL_FULL_INCLUDEDIR@/dengine-@DENGINE_VERSION@/lib -I@CMAKE_INSTALL_FULL_INCLUDEDIR@/dengine-@DENGINE_VERSION@/glue -I@CMAKE_INSTALL_FULL_INCLUDEDIR@/dengine-@DENGINE_VERSION@/deps/glad -I@CMAKE_INSTALL_FULL_INCLUDEDIR@/dengine-@DENGINE_VERSION@/deps/cglm -I@CMAKE_INSTALL_FULL_INCLUDEDIR@/dengine-@DENGINE_VERSION@/dengine_config
